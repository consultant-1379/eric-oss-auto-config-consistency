{
  "name": "eric-oss-auto-config-consistency-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "web-test-runner --playwright --debug --browsers firefox chromium",
    "test:all": "web-test-runner --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner --watch --playwright --browsers chromium",
    "start": "snowpack dev",
    "build:dev": "snowpack build",
    "build:prod": "snowpack build && rollup -c",
    "install:all": "npm install",
    "srv": "node server",
    "nuke": "rm -rf node_modules && rm -rf build",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .eslintignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .eslintignore",
    "lint:prettier": "prettier \"**/*.{js,html,css}\" --config .prettierrc --check --ignore-path .eslintignore",
    "format:prettier": "prettier \"**/*.{js,html,css}\" --config .prettierrc --write --ignore-path .eslintignore",
    "test:automation:chrome": "playwright test --project=chromium",
    "test:automation:firefox": "playwright test --project=firefox",
    "test:automation:chrome:dev": "playwright test --debug --project=chromium",
    "test:automation:firefox:dev": "playwright test --debug --project=firefox",
    "test:automation:chrome:headed": "playwright test --headed --project=chromium",
    "test:automation:firefox:headed": "playwright test --headed --project=firefox",
    "test:automation:all": "playwright test",
    "test:automation:report": "playwright show-report",
    "test:cypress": "cd integration/contract_testing && npx cypress run --spec cypress/e2e"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eui/app": "1.0.0",
    "@eui/base": "1.6.2",
    "@eui/component": "1.0.4",
    "@eui/draggable": "1.0.1",
    "@eui/container": "1.2.2",
    "@eui/layout": "1.2.11",
    "@eui/lit-component": "1.1.1",
    "@eui/navigation-menu": "1.1.3",
    "@eui/table": "1.3.10",
    "@eui/theme": "1.1.7",
    "cronstrue": "2.11.0",
    "es-module-shims": "1.5.18",
    "urlpattern-polyfill": "5.0.9"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/eslint-parser": "7.19.1",
    "@eui/import-css-plugin": "1.0.0",
    "@open-wc/eslint-config": "9.0.0",
    "@open-wc/testing": "3.1.7",
    "@open-wc/testing-helpers": "2.1.4",
    "@playwright/test": "1.33.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@snowpack/web-test-runner-plugin": "0.2.2",
    "@web/dev-server-import-maps": "0.0.7",
    "@web/test-runner": "0.15.0",
    "@web/test-runner-junit-reporter": "0.5.0",
    "@web/test-runner-playwright": "0.9.0",
    "@webcomponents/scoped-custom-element-registry": "0.0.9",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cypress": "^12.6.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-lit-a11y": "2.3.0",
    "express": "4.18.2",
    "glob": "8.0.3",
    "multer": "^1.4.5-lts.1",
    "prettier": "2.8.1",
    "playwright": "1.38.0",
    "@playwright/browser-chromium": "1.38.0",
    "@playwright/browser-firefox": "1.38.0",
    "@playwright/browser-webkit": "1.38.0",
    "rollup": "2.79.1",
    "rollup-plugin-minify-html-literals": "1.2.6",
    "rollup-plugin-summary": "1.4.3",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "15.0.0",
    "snowpack": "3.8.8",
    "wiremock": "^2.35.0"
  }
}
